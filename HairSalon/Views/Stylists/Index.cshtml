<h1>Stylists</h1>
<table class="table">
  <thead>
    <tr>
      <th>Stylist</th>
      <th>View Clients</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
  @foreach (var stylist in Model)
  {
    <tr>
      <td>@stylist.GetStylistName()</td>
      <td><a href="/stylists/@stylist.GetStylistId()"></a></td>
      <td><a href="/stylists/@stylist.GetStylistId()/delete"></a></td>
    </tr>
  }
  </tbody>
</table>
<a href="/stylists/new">Add New Stylist</a>
